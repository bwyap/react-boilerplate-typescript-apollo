{
  "name": "react-boilerplate-typescript-apollo",
  "version": "1.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/bwyap/react-boilerplate-typescript-apollo.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "author": "Ben Yap",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "yarn analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "yarn npmcheckversion",
    "prebuild": "yarn build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "yarn test && yarn build && yarn start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "yarn add chalk shelljs compare-versions",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "yarn analyze:clean && yarn test:clean && yarn build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "yarn lint:tslint:all && yarn lint:css",
    "lint:tslint": "tslint --project tsconfig.json",
    "lint:tslint:all": "tslint --project tsconfig.json --config tslint.json 'app/**/*.{ts,tsx}'",
    "lint:tslint:fix": "tslint --project tsconfig.json --config tslint.json --fix",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:staged": "lint-staged",
    "pretest": "yarn test:clean && yarn lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:only": "cross-env NODE_ENV=test jest --verbose",
    "test:ci": "cross-env NODE_ENV=test jest --coverage --ci --reporters=default --reporters=jest-junit",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:updateSnapshot": "cross-env NODE_ENV=test jest --updateSnapshot --coverage",
    "test:upload-appveyor": "node ./internals/scripts/upload-tests-appveyor",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "codecov": "codecov",
    "prettify": "prettier --write",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o build-storybook"
  },
  "jest-junit": {
    "outputDirectory": "test-results/jest",
    "outputName": "results.xml"
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
